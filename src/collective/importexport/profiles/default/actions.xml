<?xml version="1.0"?>
<object name="portal_actions" meta_type="Plone Actions Tool"
   xmlns:i18n="http://xml.zope.org/namespaces/i18n">

 <object name="object_buttons" meta_type="CMF Action Category">
  <object name="dexterity_import_button" meta_type="CMF Action" i18n:domain="collective.importexport">
   <property name="title" i18n:translate="title_dexterity_import_button">Dexterity Import</property>
   <property name="description" i18n:translate="help_dexterity_import_button">Import Dexterity objects to this context.</property>
   <property name="url_expr">string:$object_url/@@dexterity_import_view</property>
   <property name="permissions">
    <element value="collective.importexport: Import" />
   </property>
   <property name="visible">True</property>
  </object>
  <object name="dexterity_export_button" meta_type="CMF Action" i18n:domain="collective.importexport">
   <property name="title" i18n:translate="title_dexterity_export_button">Dexterity Export</property>
   <property name="description" i18n:translate="help_dexterity_export_button">Export Dexterity objects from this context.</property>
   <property name="url_expr">string:$object_url/@@dexterity_export_view</property>
   <property name="permissions">
    <element value="collective.importexport: Export" />
   </property>
   <property name="visible">True</property>
  </object>
 </object>

</object>
